<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="metadataBehavior">
          <serviceMetadata httpGetEnabled="true" httpGetUrl="http://127.0.0.1:3721/calculatorservice/metadata" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="Service.CalculatorService" behaviorConfiguration="metadataBehavior">
        <endpoint address="http://127.0.0.1:3721/calculatorservice" binding="wsHttpBinding" contract="Service.Interface.ICalculator"/>
        <endpoint address="http://127.0.0.1:3721/calculatorserv" binding="netHttpBinding" contract="Service.Interface.ICalculator"/>
      </service>
      <!--address 注意不要重复-->
      <service name="Service.FileDealServer">
        <endpoint address="http://127.0.0.1:3721/fileservice" binding="wsHttpBinding" contract="Service.Interface.IFileDeal"/>
      </service>
    </services>
  </system.serviceModel>
</configuration>